{
  "name": "bank-lending-system",
  "version": "1.0.0",
  "description": "A simple bank lending system with React frontend and Node.js backend",
  "main": "backend/server.js",
  "scripts": {
    "start": "cd backend && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && npm run dev",
    "client": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "init-db": "cd backend && npm run init-db",
    "reset-db": "cd backend && npm run reset-db"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "bank",
    "lending",
    "loan",
    "emi",
    "react",
    "nodejs",
    "express"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
